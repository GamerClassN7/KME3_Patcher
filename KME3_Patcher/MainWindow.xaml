<Window x:Class="KME3_Patcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KME3_Patcher"
        mc:Ignorable="d"
        Title="KME3_Patcher" Height="136" Width="246" ContentRendered="Window_ContentRendered">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="PatchGame" Content="Patch" Margin="10,5,69,7" Click="Button_Click" Grid.Row="2"/>
        <TextBox x:Name="ServerAddress" Margin="10,5,10,4" TextWrapping="Wrap" Text="Server Address"/>
        <Button x:Name="PatchGame_Copy" Content="Undo" Margin="182,6,10,7" Click="Button_Click" Grid.Row="2"/>
        <Label Content="Status" Margin="10,5,10,6" Grid.Row="1"/>
    </Grid>
</Window>

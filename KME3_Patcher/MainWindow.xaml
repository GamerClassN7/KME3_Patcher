<Window x:Class="KME3_Patcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KME3_Patcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="167" Width="246" ContentRendered="Window_ContentRendered">
    <Grid>
        <Button x:Name="PatchGame" Content="Patch" Margin="10,0,10,10" Click="Button_Click" Height="28" VerticalAlignment="Bottom"/>
        <TextBox x:Name="ServerAddress" Margin="10,10,10,0" TextWrapping="Wrap" Text="TextBox" Height="31" VerticalAlignment="Top"/>
    </Grid>
</Window>
